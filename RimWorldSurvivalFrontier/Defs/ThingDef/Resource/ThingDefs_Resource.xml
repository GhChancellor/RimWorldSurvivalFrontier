<?xml version="1.0" encoding="UTF-8"?>

<Defs>
    <!--  ===== RSF_PlasticBasic > ThingCategoryDef TradingGoods ==== -->
    <ThingDef ParentName="ResourceVerbBase">
        <!-- ThingDefs_Items/Items_Resource_Stuff.xml > WoodLog -->
        <!-- CORE IDENTITY -->
        <defName>RSF_PlasticBasic</defName>
        <label>RSF_PlasticBasic.label</label>
        <description>RSF_PlasticBasic.description</description>

        <!-- VISUAL REPRESENTATION -->
        <graphicData>
            <texPath>Things/Item/Resource/StoneBlocks</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
            <color>(255,255,255)</color> <!-- This is the color of the icon that you can see in the inventory! -->
            <!-- see: EcoPlastic/Doc/Images/ThingDef/IconColor.png-->
        </graphicData>
        
        <equippedAngleOffset>-20</equippedAngleOffset> 
        
        <!-- PHYSICAL PROPERTIES -->
        <soundInteract>Wood_Drop</soundInteract>
        <soundDrop>Wood_Drop</soundDrop>
        
        <!-- STATISTICS -->
        <statBases>
            <!-- Economic: Slightly cheaper than wood (1.2 vs 1.02) -->
            <MarketValue>1.02</MarketValue>

            <!-- Durability: Reduced hitpoints, bioplastic is less sturdy -->
            <MaxHitPoints>127</MaxHitPoints>

            <!-- Weight: Lighter than wood, easier transport -->
            <Mass>0.3</Mass>

            <!-- Fire: Slightly less flammable than wood -->
            <Flammability>0.9</Flammability>

            <!-- Weather: Better resistance to deterioration -->
            <DeteriorationRate>0.4</DeteriorationRate>

            <!-- Combat: Poor armor values - not suitable for military gear -->
            <StuffPower_Armor_Sharp>0.25</StuffPower_Armor_Sharp>
            <StuffPower_Armor_Blunt>0.25</StuffPower_Armor_Blunt>
            <StuffPower_Armor_Heat>0.20</StuffPower_Armor_Heat>

            <!-- Insulation: Same as wood - could be improved for extreme biomes -->
            <StuffPower_Insulation_Cold>8</StuffPower_Insulation_Cold>
            <StuffPower_Insulation_Heat>4</StuffPower_Insulation_Heat>

            <!-- Weapon damage: Very poor for combat applications -->
            <SharpDamageMultiplier>0.20</SharpDamageMultiplier>
            <BluntDamageMultiplier>0.5</BluntDamageMultiplier>
        </statBases>
        
        <burnableByRecipe>true</burnableByRecipe> <!-- Can be used as fuel -->
        <healthAffectsPrice>false</healthAffectsPrice> <!-- No effect on price if damaged -->
        <minRewardCount>10</minRewardCount> <!-- Quest rewards -->
        
        <stuffProps>
            <!-- Material appearance and naming -->
<!--            <stuffAdjective>bioplastic</stuffAdjective>-->
            <!-- Material categories this can be used for -->
            <categories>
                <li>Woody</li> <!-- Furniture material -->
            </categories>
            <commonality>0.85</commonality> <!-- Spawn probability (wood 1.0)-->
            <color>(255,255,255)</color> <!-- This is the color of the icon that you can see in the menu and ingame! -->
            <!-- see: EcoPlastic/Doc/Images/ThingDef/IconcolorMenuAndGame.png-->

            <!-- Audio/Visual effects - using wood equivalents (closest vanilla match) -->
            <constructEffect>ConstructWood</constructEffect>
<!--            <appearance>Planks</appearance> &lt;!&ndash; Maintains plank appearance like wood &ndash;&gt;-->
            
            <!-- Sound effects for interactions -->
            <soundImpactBullet>BulletImpact_Wood</soundImpactBullet>  <!-- Bullet impact sound -->
            <soundMeleeHitSharp>MeleeHit_Wood</soundMeleeHitSharp>    <!-- Sharp weapon hit sound -->
            <soundMeleeHitBlunt>MeleeHit_Wood</soundMeleeHitBlunt>    <!-- Blunt weapon hit sound -->
            <soundImpactMelee>Pawn_Melee_Punch_HitBuilding_Wood</soundImpactMelee>  <!-- Melee impact sound -->
            
            <statFactors>
                <MaxHitPoints>0.65</MaxHitPoints>
                <Beauty>0.8</Beauty> <!-- Less attractive than natural materials -->
                <Flammability>0.95</Flammability> <!-- Multiplied by fire (Flammability * Flammability) -->
                <WorkToMake>0.7</WorkToMake> <!-- How to craft items ( dresses, weapons.... ) from this material -->
                <WorkToBuild>0.7</WorkToBuild> <!-- How to build structures ( wall, door ) from this material -->
                <DoorOpenSpeed>1.2</DoorOpenSpeed>
            </statFactors>
            <canSuggestUseDefaultStuff>false</canSuggestUseDefaultStuff>
        </stuffProps>
        <isTechHediff>true</isTechHediff> <!-- can use it to build prosthesis limb -->
        
        <!-- Inventory category -->
        <thingCategories>
            <li>ResourcesRaw</li>
            <li>TradingGoods</li> <!-- Custom category for traders see: PatchTraders.xml > categoryDef-->
        </thingCategories>
        
        <techHediffsTags>
            <li>Poor</li> <!-- quality of prosthesis -->
        </techHediffsTags>
        <weaponClasses>
            <li>Melee</li>
            <li>MeleeBlunt</li>
            <li>Neolithic</li>
        </weaponClasses>
        <tools>
            <li>
                <label>log</label>
                <capacities>
                    <li>Blunt</li>
                </capacities>
                <power>8</power>
                <cooldownTime>2.5</cooldownTime>
            </li>
        </tools>
        <terrainAffordanceNeeded>Light</terrainAffordanceNeeded>
        <allowedArchonexusCount>150</allowedArchonexusCount>
    </ThingDef>

