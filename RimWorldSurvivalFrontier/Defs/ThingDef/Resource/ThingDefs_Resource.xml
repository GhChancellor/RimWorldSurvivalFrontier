    <!--  ===== RSF_PlasticBioSteel > ThingCategoryDef TradingGoods ==== -->
    <ThingDef ParentName="ResourceBase">
        <!--ThingDefs_Items/Items_Resource_Stuff.xml > Plasteel-->
        <!-- CORE IDENTITY -->
        <defName>RSF_PlasticBioSteel</defName>
        <label>RSF_PlasticBioSteel.label</label>
        <description>RSF_PlasticBioSteel.description</description>

        <!-- VISUAL REPRESENTATION -->
        <graphicData>
            <texPath>Things/Item/Resource/Plasteel</texPath>
            <graphicClass>Graphic_StackCount</graphicClass>
            <color>(30,144,255)</color> <!-- This is the color of the icon that you can see in the inventory! -->
            <!-- see: EcoPlastic/Doc/Images/ThingDef/IconColor.png-->
        </graphicData>

        <!-- SOUND EFFECTS -->
        <soundInteract>Metal_Drop</soundInteract>
        <soundDrop>Metal_Drop</soundDrop>
        <useHitPoints>false</useHitPoints>
        <healthAffectsPrice>false</healthAffectsPrice>                 <!-- No effect on price if damaged -->
        
        <!-- RAW MATERIAL STATS -->
        <statBases>
            <!-- Economic values -->
            <MarketValue>5.5</MarketValue>                             <!-- Industrial grade, more expensive than wood -->
            <Mass>0.28</Mass>                                          <!-- Denser than RSF_PlasticBasic -->

            <!-- Armor effectiveness when used as stuff -->
            <StuffPower_Armor_Sharp>0.95</StuffPower_Armor_Sharp>     <!-- Better protection than basic -->
            <StuffPower_Armor_Blunt>0.46</StuffPower_Armor_Blunt>     <!-- Enhanced blunt protection -->
            <StuffPower_Armor_Heat>0.55</StuffPower_Armor_Heat>       <!-- Still melts under heat -->

            <!-- Insulation effectiveness when used as stuff -->
            <StuffPower_Insulation_Cold>2.4</StuffPower_Insulation_Cold> <!-- Better cold insulation -->
            <StuffPower_Insulation_Heat>0</StuffPower_Insulation_Heat> <!-- Better heat insulation -->

            <!-- Damage multipliers for weapons made from this ???? -->
            <!-- Da rivedere-->
            <SharpDamageMultiplier>0.93</SharpDamageMultiplier>       <!-- Better than basic but still fragile -->
            <BluntDamageMultiplier>0.7</BluntDamageMultiplier>        <!-- More rigid than basic -->
        </statBases>

        <isTechHediff>true</isTechHediff> <!-- can use it to build prosthesis limb -->

        <!-- Inventory category -->
        <thingCategories>
            <li>ResourcesRaw</li>
            <li>TradingGoods2</li>
        </thingCategories>

        <techHediffsTags>
            <li>Simple</li> <!-- quality of prosthesis -->
        </techHediffsTags>

<!--        <deepCommonality>1</deepCommonality>-->
<!--        <deepCountPerPortion>10</deepCountPerPortion>-->
<!--        <deepLumpSizeRange>2~10</deepLumpSizeRange>-->
<!--        <burnableByRecipe>false</burnableByRecipe>-->
<!--        <smeltable>true</smeltable>-->

        <stuffProps>
            <categories>
                <!-- Material categories this can be used for -->
                <li>Metallic</li> <!-- Industrial material strutucture, weapons, etc. -->
            </categories>
            <appearance>Metal</appearance>
            <commonality>0.3</commonality>
            <constructEffect>ConstructMetal</constructEffect>
            <isAirtight>true</isAirtight>
            <color>(30,144,255)</color> <!-- This is the color of the icon that you can see in the menu and ingame! -->
            <!-- see: EcoPlastic/Doc/Images/ThingDef/IconcolorMenuAndGame.png-->
            
            <!-- Sound effects -->
            <soundImpactBullet>BulletImpact_Metal</soundImpactBullet>
            <soundMeleeHitSharp>MeleeHit_Metal_Sharp</soundMeleeHitSharp>
            <soundMeleeHitBlunt>MeleeHit_Metal_Blunt</soundMeleeHitBlunt>
            <soundImpactMelee>Pawn_Melee_Punch_HitBuilding_Metal</soundImpactMelee>
            
            <statFactors>
                <MaxHitPoints>2.2</MaxHitPoints>
                <Beauty>1.3</Beauty>
                <Flammability>0.3</Flammability>
                <WorkToMake>1.4</WorkToMake>
                <WorkToBuild>1.4</WorkToBuild>
                <MeleeWeapon_CooldownMultiplier>0.8</MeleeWeapon_CooldownMultiplier> <!-- ????-->
            </statFactors>
        </stuffProps>

        <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
        <allowedArchonexusCount>80</allowedArchonexusCount>
        
    </ThingDef>

