<?xml version="1.0" encoding="utf-8"?>

<Defs>

    <!--  ================= RSF_ChemfuelFromPlasticRecipe  ================= -->
    <RecipeDef>
        <!-- RecipeDefs/Recipes_Production_Vanilla.xml > Make_ChemfuelFromWood -->
        <!-- ID -->
        <defName>RSF_ChemfuelFromPlasticRecipe</defName>
        <label>RSF_ChemfuelFromPlasticRecipe.label</label>
        <description>RSF_ChemfuelFromPlasticRecipe.description</description>

        <!-- Mechanic CORE -->
        <jobString>RSF_ChemfuelFromPlasticRecipe.jobString</jobString>
        <workAmount>2000</workAmount>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat> <!-- Work Speed Stat -->
        <researchPrerequisite>RSF_PlasticBasicResearch</researchPrerequisite>

        <!-- Preserves work progress if interrupted -->
        <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
        <displayPriority>0</displayPriority> <!-- priority work-->

        <!-- Where you craft it  -->
        <recipeUsers>
            <li>BiofuelRefinery</li>
        </recipeUsers>

        <!-- INGREDIENTS -->
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>RSF_PlasticBasic</li>
                        <li>RSF_PlasticIndustrial</li>
                        <li>RSF_PlasticFiber</li>
                        <li>RSF_PlasticIncendiary</li>
                        <li>RSF_EcoLeather</li>
                    </thingDefs>
                </filter>
                <count>50</count>
            </li>
        </ingredients>

        <!-- Allow mixing different plant ingredients in same recipe -->
        <allowMixingIngredients>true</allowMixingIngredients>

        <!-- Default UI filter - enables all Plastics by default -->
        <fixedIngredientFilter>
            <thingDefs>
                <li>RSF_PlasticBasic</li>
                <li>RSF_PlasticIndustrial</li>
                <li>RSF_PlasticFiber</li>
                <li>RSF_PlasticIncendiary</li>
                <li>RSF_EcoLeather</li>
            </thingDefs>
        </fixedIngredientFilter>

        <products>
            <Chemfuel>40</Chemfuel>
        </products>

        <!-- Effect Working & sound -->
        <effectWorking>Cremate</effectWorking>
        <soundWorking>Recipe_Cremate</soundWorking>
    </RecipeDef>

    <!--  ================= RSF_PlasticBasicRecipe  ================= -->
    <RecipeDef>
        <!-- RecipeDefs/Recipes_Production_Vanilla.xml > Make_ChemfuelFromWood -->
        <!-- ID -->
        <defName>RSF_PlasticBasicRecipe</defName>
        <label>RSF_PlasticBasicRecipe.label</label>
        <description>RSF_PlasticBasicRecipe.description</description>

        <!-- Mechanic CORE -->
        <jobString>RSF_PlasticBasicRecipe.jobString</jobString>
        <workAmount>800</workAmount>
        <workSpeedStat>GeneralLaborSpeed</workSpeedStat> <!-- Work Speed Stat -->
        <workSkill>Crafting</workSkill> <!-- Work XP Skill -->
        <researchPrerequisite>RSF_PlasticBasicResearch</researchPrerequisite> <!-- unlock this items See: EcoPlastic/Doc/Images/Research/Unlock_RSF_PlasticBasic.png -->

        <!-- Requirements -->
        <skillRequirements>
            <Crafting>5</Crafting>
        </skillRequirements>

        <!-- Preserves work progress if interrupted -->
        <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>
        <!--        <displayPriority>0</displayPriority> &lt;!&ndash; priority work&ndash;&gt;-->

        <!-- Where you craft it  -->
        <recipeUsers>
            <li>BiofuelRefinery</li>
        </recipeUsers>

        <!-- INGREDIENTS -->
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li MayRequire="Dubwise.DubsBadHygiene">FecalSludge</li>
                    </thingDefs>
                    <categories>
                        <li>PlantFoodRaw</li>
                    </categories>
                    <disallowedThingDefs>
                        <li>RawFungus</li>
                    </disallowedThingDefs>
                </filter>
                <count>50</count>
            </li>
        </ingredients>

        <!-- Allow mixing different plant ingredients in same recipe -->
        <allowMixingIngredients>true</allowMixingIngredients>

        <!-- Default UI filter - enables PlantFoodRaw by default -->
        <defaultIngredientFilter>
            <thingDefs>
                <li MayRequire="Dubwise.DubsBadHygiene">FecalSludge</li>
            </thingDefs>
            <categories>
                <li>PlantFoodRaw</li>
            </categories>
        </defaultIngredientFilter>

        <!-- Plant food filtering - excludes fungi and drugs -->
        <fixedIngredientFilter>
            <thingDefs>
                <li MayRequire="Dubwise.DubsBadHygiene">FecalSludge</li>
            </thingDefs>
            <categories>
                <li>PlantFoodRaw</li>
            </categories>
            <disallowedThingDefs>
                <li>RawFungus</li>
            </disallowedThingDefs>
        </fixedIngredientFilter>

        <!-- OUTPUT material-->
        <products>
            <RSF_PlasticBasic>45</RSF_PlasticBasic>
        </products>

        <!-- Effect AUDIO/VISUAL -->
        <effectWorking>Cremate</effectWorking>
        <soundWorking>Recipe_Cremate</soundWorking>
    </RecipeDef>

