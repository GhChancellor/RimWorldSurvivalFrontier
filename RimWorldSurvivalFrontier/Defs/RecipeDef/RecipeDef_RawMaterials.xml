        <recipeUsers>
            <li>FabricationBench</li> <!-- Fabrication bench -->
        </recipeUsers>

        <!-- INGREDIENTS -->
        <ingredients>
            <li>
                <filter>
                    <thingDefs>
                        <li>Steel</li>
                    </thingDefs>
                </filter>
                <count>20</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>RSF_PlasticIndustrial</li>
                    </thingDefs>
                </filter>
                <count>18</count>
            </li>
            <li>
                <filter>
                    <thingDefs>
                        <li>Chemfuel</li>
                    </thingDefs>
                </filter>
                <count>4</count>
            </li>
        </ingredients>

        <!-- Steel filtering -->
        <fixedIngredientFilter>
            <thingDefs>
                <li>Steel</li>
                <li>RSF_PlasticIndustrial</li>
                <li>Chemfuel</li>
            </thingDefs>
        </fixedIngredientFilter>

        <!-- OUTPUT material-->
